model User{
  id Int @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  email String
  password String

  firstName String?
  lastName String?

}


model Bookmark{
  id Int @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  title String
  description String?
  link String
}

npx prisma generate 
npx prisma studio 
npx prisma migrate dev --name init
npx prisma db push

npx prisma migrate deploy